#include <bits/stdc++.h> 
int f(int ind,vector<int> &heights,vector<int> &dp){
    if(ind==0){
        return 0;
    }
    if(dp[ind]!=-1){
        return dp[ind];
    }
    int left,right=INT_MAX;
    left=f(ind-1,heights,dp)+abs(heights[ind]-heights[ind-1]);
    if(ind>1){
        right=f(ind-2,heights,dp)+abs(heights[ind]-heights[ind-2]);
    }
    return dp[ind]=min(left,right);
}
int frogJump(int n, vector<int> &heights)
{
    int a=0;
    int b=0;
    int curri;
    for(int i=1;i<n;i++){
        int fs=b+abs(heights[i]-heights[i-1]);
        int ss=INT_MAX;
        if (i > 1) {
           ss = a + abs(heights[i] - heights[i - 2]);
        }
        curri=min(fs,ss);
        a=b;
        b=curri;
    }
    return b;
}
